<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#80ffffe0" />
            <stroke android:width="0.5dip" android:color="#ffffb525" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#80e1e1e1" />
            <stroke android:width="0.5dip" android:color="#25e1e1e1" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffffe0" />
            <stroke android:width="0.5dip" android:color="#ffffb525" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>
